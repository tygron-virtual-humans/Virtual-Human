module HousingMain{
	program{
		if goal(buildHouses(X,Y)) then build(X,Y) + drop(buildHouses(X,Y)).
		if goal(buyNewLand(Need,X)) then buyLand(Need,X) + drop(buyNewLand(Need,X)).
		
		if bel( land(Surface), Surface = 0, calculateNeededLand(50,Need), standardPrice(X)) then adopt(buyNewLand(Need,X)).
		if bel(land(X), X>0) then adopt(buildHouses(X,1)).
	
	}
}